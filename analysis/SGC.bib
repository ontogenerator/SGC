	pmcid = {PMC3365199},
	keywords = {Animals, Mice, Reinforcement (Psychology), Choice Behavior, Models, Psychological, Exploratory Behavior, Algorithms, Discrimination Learning, Mice, Inbred C57BL},
	pages = {8776--8779},
}


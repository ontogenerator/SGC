}

@article{iverson_analytical_2006-1,
	title = {Analytical methods in the theory of psychophysical discrimination {II}: {The} {Near}-miss to {Weber}'s {Law}, {Falmagne}'s {Law}, the {Psychophysical} {Power} {Law} and the {Law} of {Similarity}},

	file = {PubMed Central Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\EMCGEB4P\\Chappell and Hawes - 2012 - Biological and artificial cognition what can we l.pdf:application/pdf},
}

@article{brandstatter_priority_2006,
	title = {The {Priority} {Heuristic}: {Making} {Choices} {Without} {Trade}-{Offs}},

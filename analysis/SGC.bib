	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/bimj.200810425},
	keywords = {Multiple comparisons, Adjusted p -values, Multiple tests, Multivariate normal distribution, Robust statistics, Simultaneous confidence intervals},
	pages = {346--363},
	file = {Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\MHJDPNYG\\bimj.html:text/html;Full Text:C\:\\Users\\Vladi\\Zotero\\storage\\4F22X3BN\\Hothorn et al. - 2008 - Simultaneous Inference in General Parametric Model.pdf:application/pdf},

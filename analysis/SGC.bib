	pages = {20151439},
	file = {Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\HPHXBJ8B\\Oud et al. - 2016 - Irrational time allocation in decision-making.pdf:application/pdf;Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\RQ56IRBN\\20151439.html:text/html},

	file = {ScienceDirect Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\5TMVG5CQ\\Richter and Waddington - 1993 - Past foraging experience influences honey bee danc.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\QQCTHXS6\\Richter and Waddington - 1993 - Past foraging experience influences honey bee danc:},
}


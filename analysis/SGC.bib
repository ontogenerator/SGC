	keywords = {Animals, Reproduction, Male, Female, Flowers, Biological Evolution, Behavior, Animal, Computer Simulation, Bees, Birds, Color, Food Preferences, Pigments, Biological, Plant Physiological Phenomena, Plants, Pollen},
	pages = {551--566},
}


	shorttitle = {Hummingbirds' nectar concentration preferences at low volume},
	url = {http://www.sciencedirect.com/science/article/B6W9W-45N4R8S-3G/2/1f48d57d14373ffc3ef39918ccdccd1e},
	doi = {10.1006/anbe.1996.0180},
	abstract = {Although average nectar sugar concentrations in hummingbird-pollinated plants are low (20-25\% sucrose-equivalent sugars; mass/total mass), in choice tests birds have preferred concentrations higher than 45\%. This discrepancy might be explained by the use of large nectar volumes in all published choice tests to date. Biophysical models predict that low concentrations maximize energy intake rates, and thus may be preferred, at small nectar volumes characteristic of the flowers that birds visit. Rufous hummingbirds',Selasphorus rufus, concentration preferences were tested and their energy intake rates measured at low volume with a computer-controlled food-delivery and activity-monitoring system. Contrary to the predictions of models, birds generally preferred 65\% sucrose over more dilute solutions. This preference generated near-maximal energy intake rates over the time scale of foraging bouts, even though rates over flower visits and shorter time scales suggested that lower concentrations would be more profitable. Neither hummingbirds' preferences nor the energetics of nectar extraction in this low-volume presentation account for the low nectar sugar concentrations characteristic of the flowers they pollinate.},
	number = {2},
	urldate = {2011-02-06},
	journal = {Animal Behaviour},
	author = {Roberts, Mark W.},
	month = aug,
	year = {1996},
	pages = {361--370},
}


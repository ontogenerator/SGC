@article{trimmer_decision-making_2011,
	title = {Decision-making under uncertainty: biases and {Bayesians}},
	volume = {14},
	issn = {1435-9448, 1435-9456},
	shorttitle = {Decision-making under uncertainty},
	url = {http://link.springer.com/article/10.1007/s10071-011-0387-4},
	doi = {10.1007/s10071-011-0387-4},
	abstract = {Animals (including humans) often face circumstances in which the best choice of action is not certain. Environmental cues may be ambiguous, and choices may be risky. This paper reviews the theoretical side of decision-making under uncertainty, particularly with regard to unknown risk (ambiguity). We use simple models to show that, irrespective of pay-offs, whether it is optimal to bias probability estimates depends upon how those estimates have been generated. In particular, if estimates have been calculated in a Bayesian framework with a sensible prior, it is best to use unbiased estimates. We review the extent of evidence for and against viewing animals (including humans) as Bayesian decision-makers. We pay particular attention to the Ellsberg Paradox, a classic result from experimental economics, in which human subjects appear to deviate from optimal decision-making by demonstrating an apparent aversion to ambiguity in a choice between two options with equal expected rewards. The paradox initially seems to be an example where decision-making estimates are biased relative to the Bayesian optimum. We discuss the extent to which the Bayesian paradigm might be applied to the evolution of decision-makers and how the Ellsberg Paradox may, with a deeper understanding, be resolved.},
	language = {en},
	number = {4},
	urldate = {2014-01-30},
	journal = {Animal Cognition},
	author = {Trimmer, Pete C. and Houston, Alasdair I. and Marshall, James A. R. and Mendl, Mike T. and Paul, Elizabeth S. and McNamara, John M.},
	month = jul,
	year = {2011},
	keywords = {Risk, Behavioural Sciences, evolutionary biology, Human Physiology, Zoology, Uncertainty, Ambiguity, Animal decisions, Cognitive bias, Ellsberg Paradox},
	pages = {465--476},
	file = {Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\BI4C6987\\Trimmer et al. - 2011 - Decision-making under uncertainty biases and Baye.pdf:application/pdf;Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\NXNRBI2U\\10.html:text/html},

@article{vasconcelos_transitive_2008,
	title = {Transitive inference in non-human animals: {An} empirical and theoretical analysis},
	volume = {78},
	issn = {0376-6357},
	shorttitle = {Transitive inference in non-human animals},
	url = {http://www.sciencedirect.com/science/article/pii/S0376635708000818},
	doi = {10.1016/j.beproc.2008.02.017},
	abstract = {Transitive inference has long been considered one of the hallmarks of human deductive reasoning. Recent reports of transitive-like behaviors in non-human animals have prompted a flourishing empirical and theoretical search for the mechanism(s) that may mediate this ability in non-humans. In this paper, I begin by describing the transitive inference tasks customarily used with non-human animals and then review the empirical findings. Transitive inference has been demonstrated in a wide variety of species, and the signature effects that usually accompany transitive inference in humans (the serial position effect and the symbolic distance effect) have also been found in non-humans. I then critically analyze the most prominent models of this ability in non-human animals. Some models are cognitive, proposing for instance that animals use the rules of formal logic or form mental representations of the premises to solve the task, others are based on associative mechanisms such as value transfer and reinforcement and non-reinforcement. Overall, I argue that the reinforcement-based models are in a much better empirical and theoretical position. Hence, transitive inference in non-human animals should be considered a property of reinforcement history rather than of inferential processes. I finalize by shedding some light on some promising lines of research.},
	number = {3},
	urldate = {2013-10-16},
	journal = {Behavioural Processes},
	author = {Vasconcelos, Marco},
	month = jul,
	year = {2008},
	keywords = {Cognitive models, Reinforcement-based models, Serial position effect, Symbolic distance effect, transitive inference, Value transfer theory},
	pages = {313--334},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\ASCTKQ3U\\Vasconcelos - 2008 - Transitive inference in non-human animals An empi.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\A7WUU42W\\Vasconcelos - 2008 - Transitive inference in non-human animals An empi.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\879Z8PS3\\S0376635708000818.html:text/html;ScienceDirect Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\SPQRNJNI\\S0376635708000818.html:text/html},
}

@article{van_de_pol_quantifying_2012,
	title = {Quantifying individual variation in reaction norms: how study design affects the accuracy, precision and power of random regression models},
	volume = {3},
	copyright = {© 2011 The Author. Methods in Ecology and Evolution © 2011 British Ecological Society},
	issn = {2041-210X},
	shorttitle = {Quantifying individual variation in reaction norms},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.2041-210X.2011.00160.x/abstract},
	doi = {10.1111/j.2041-210X.2011.00160.x},
	abstract = {1. Quantifying individual heterogeneity in plasticity is becoming common in studies of evolutionary ecology, climate change ecology and animal personality. Individual variation in reaction norms is typically quantified using random effects in a mixed modelling framework. However, little is known about what sampling effort and design provide sufficient accuracy, precision and power.2. I developed ‘odprism’, an easy-to-use software package for the statistical language R, which can be used to investigate the accuracy, precision and power of random regression models for various types of data structures. Moreover, I conducted simulations to derive rules-of-thumb for four design decisions that biologists often face.3. First, I investigated the trade-off between sampling many individuals a few times versus sampling few individuals often. Generally, at least 40 individuals should be sampled with a total sample size of at least 1000 to obtain accurate and precise estimates of individual variation in elevation and slopes of linear reaction norms and their correlation. Contrasting a previous recommendation, it is worthwhile to bias the ratio of number of individuals over replicates towards sampling more individuals.4. Second, I considered how the range of environmental conditions over which individuals are sampled affects the optimal sampling strategy. I show that when all individuals experience the same conditions during a sampling event, sampling each individual only twice should be strictly avoided.5. Third, I examined the case where the number of replicates per individual is constrained by their lifespan, as is common when sampling annual traits in the wild. I show that for a given sampling effort, it is much easier to detect individual variation in reaction norms for long-lived than for short-lived species.6. Fourth, I investigated the performance of random regression models when studying traits under selection. Reassuringly, directional viability selection barely caused any bias in estimates of variance components.7. Random regression models are inherently data hungry, and reviewing the literature shows that particularly behavioural studies have low sampling effort. Therefore, the software and rules-of-thumbs I identified for designing reaction-norm studies should help researchers make more informed choices, which likely improve the reliability and interpretation of plasticity studies.},
	language = {en},
	number = {2},
	urldate = {2013-04-24},
	journal = {Methods in Ecology and Evolution},
	author = {van de Pol, Martijn},
	year = {2012},
	keywords = {phenotypic plasticity, STATISTICS, annual traits, bias, I × E, life-history, lifespan, mixed model, random slopes, viability selection},
	pages = {268--280},
	file = {Snapshot:C\:\\Users\\Vladi\\Zotero\\storage\\63VSTK2X\\full.html:text/html;Full Text PDF:C\:\\Users\\Vladi\\Zotero\\storage\\QJX5XF6H\\van de Pol - 2012 - Quantifying individual variation in reaction norms.pdf:application/pdf},
}

